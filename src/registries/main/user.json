{
    "username": {
        "inputQuery": "#username",
        "inputClass": "text",
        "pattern": "^\\w{3,15}$",
        "value": null
    },
    "roleId": {
        "inputQuery": "#roleId",
        "inputClass": "dropDown",
        "pattern": "^[0-9]{1,}$",
        "value": null
    },
    "employeeCode": {
        "inputQuery": "#employeeCode",
        "inputClass": "text",
        "pattern": "^EMP[0-9]{7}$",
        "value": null
    },
    "addedDate": {
        "inputQuery": "#addedDate",
        "inputClass": "text",
        "pattern": "\\d{4}[-]\\d{2}[-]\\d{2}$",
        "value": null
    }
}