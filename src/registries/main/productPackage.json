{
    "code": {
        "inputQuery": "#code",
        "inputClass": "text",
        "pattern": "^PKG[0-9]{7}$",
        "value": null
    },
    "photo": {
        "inputQuery": "#photo",
        "inputClass": "image",
        "pattern": "62000;image/png,image/jpg",
        "size": null,
        "type": "image/png",
        "value": null
    },
