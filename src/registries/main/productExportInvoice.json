{
    "code": {
        "inputQuery": "#code",
        "inputClass": "text",
        "pattern": "^PEI[0-9]{7}$",
        "value": null
    },
    "requestCode": {
        "inputQuery": "#requestCode",
        "inputClass": "text",
        "pattern": "^PER[0-9]{7}$",
        "value": null
    },
    "price": {
        "inputQuery": "#price",
        "inputClass": "text",
        "pattern": "^\\d{1,7}[.]\\d{2}$",
        "value": null
    },
    "discountPercentage": {
        "inputQuery": "#discountPercentage",
        "inputClass": "text",
        "pattern": "^[0-9]{1,2}$",
        "value": null
    },
    "finalPrice": {
        "inputQuery": "#finalPrice",
        "inputClass": "text",
        "pattern": "^\\d{1,7}[.]\\d{2}$",
        "value": null
    },
    "invoiceStatusId": {
        "inputQuery": "#invoiceStatusId",
        "inputClass": "dropDown",
        "pattern": "^[1-9]{1,}$",
        "value": null
    },
    "description": {
        "inputQuery": "#description",
        "inputClass": "textarea",
        "pattern": null,
        "value": ""
    },
    "addedDate": {
        "inputQuery": "#addedDate",
        "inputClass": "text",
        "pattern": "\\d{4}[-]\\d{2}[-]\\d{2}$",
        "value": null
    }
}