{
    "code": {
        "inputQuery": "#code",
        "inputClass": "text",
        "pattern": "^MBT[0-9]{7}$",
        "value": null
    },
    "materialId": {
        "inputQuery": "#materialId",
        "inputClass": "dropDown",
        "pattern": "^[1-9]{1,}$",
        "value": null
    },
    "amount": {
        "inputQuery": "#amount",
        "inputClass": "text",
        "pattern": "^\\d{1,}[.]\\d{1,}$",
        "value": null
    },
    "viablePeriod": {
        "inputQuery": "#viablePeriod",
        "inputClass": "text",
        "pattern": "^\\d{1,}$",
        "value": null
    },
    "batchStatusId": {
        "inputQuery": "#batchStatusId",
        "inputClass": "dropDown",
        "pattern": "^[1-9]{1,}$",
        "value": null
    },
    "description": {
        "inputQuery": "#description",
        "inputClass": "textarea",
        "pattern": null,
        "value": ""
    },
    "addedDate": {
        "inputQuery": "#addedDate",
        "inputClass": "text",
        "pattern": "\\d{4}[-]\\d{2}[-]\\d{2}$",
        "value": null
    }
}